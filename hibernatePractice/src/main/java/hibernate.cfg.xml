<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC  
          "-//Hibernate/Hibernate Configuration DTD 5.3//EN"  
          "http://hibernate.sourceforge.net/hibernate-configuration-5.3.dtd">  
  
<hibernate-configuration>  
  
    <session-factory>  
        <property name="hbm2ddl.auto">update</property> 
        <property name="show_sql">true</property> 
        <property name="dialect">org.hibernate.dialect.MySQL8Dialect</property>  
        <property name="connection.url">jdbc:mysql://localhost:3306/hibernate_practice</property>  
        <property name="connection.username">root</property>  
        <property name="connection.password">root</property>  
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>  
    	<mapping class="com.durgendra.hbmapping.CorporateDetail"/>  
    	<mapping class="com.durgendra.hbmapping.InvoiceEntry"/>  
    	<mapping class="com.durgendra.hbmapping.InvoiceEntryDetail"/>  
    	<mapping class="com.durgendra.hbmapping.onetoone.CorporateDetailOTO"/>  
    	<mapping class="com.durgendra.hbmapping.onetoone.InvoiceEntryOTO"/> 
    	<mapping class="com.durgendra.hbmapping.onetoone.InvoiceEntryDetailOTO"/> 
    	<mapping class="com.durgendra.hbmapping.onetomany.CorporateDetailOTM"/>  
    	<mapping class="com.durgendra.hbmapping.onetomany.InvoiceEntryOTM"/> 
    	<mapping class="com.durgendra.hbmapping.onetomany.InvoiceEntryDetailOTM"/> 
    	<mapping class="com.durgendra.hbmapping.manytoone.Address"/> 
    	<mapping class="com.durgendra.hbmapping.manytoone.Employee"/> 
    </session-factory>  
  
</hibernate-configuration>